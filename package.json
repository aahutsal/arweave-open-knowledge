{
  "name": "arweave-open-knowledge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "glob": "^7.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "npx react-scripts start",
    "eject": "npx react-scripts eject",
    "build": "npm run build:react && npm run build:bundle",
    "build:react": "npx react-scripts build",
    "build:bundle": "npx webpack --config webpack.config.js",
    "deploy": "npm run build && npm run deploy:arweave",
    "deploy:arweave": "npx arweave deploy",
    "test:jest": "npx jest",
    "test:ci": "npx react-scripts test",
    "test": "npm run test:ci && npm run test:jest",
    "ci:before": "npx greenkeeper-lockfile-update",
    "ci:after": "npx greenkeeper-lockfile-upload",
    "release": "standard-version",
    "clean": "rm -rf dist/*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "arweave-deploy": "^1.5.0",
    "css-loader": "^2.1.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.5"
  }
}
